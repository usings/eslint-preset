{
  "name": "eslint-presets",
  "version": "2.3.0",
  "type": "module",
  "description": "ESLint presets",
  "author": "Joie <leaps@live.com>",
  "license": "MIT",
  "homepage": "https://github.com/usings/eslint-preset/tree/main/packages/eslint-presets",
  "keywords": [
    "eslint-config",
    "eslint-flat-config",
    "eslint-preset"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./configs": {
      "import": "./dist/configs.mjs",
      "types": "./dist/configs.d.ts"
    },
    "./modules": {
      "import": "./dist/modules.mjs",
      "types": "./dist/modules.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "eslint-preset-astro": "workspace:*",
    "eslint-preset-basic": "workspace:*",
    "eslint-preset-react": "workspace:*",
    "eslint-preset-solid": "workspace:*",
    "eslint-preset-svelte": "workspace:*",
    "eslint-preset-vue": "workspace:*"
  },
  "peerDependencies": {
    "eslint": ">=8.28.0"
  }
}
